{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.3",
        "arcanedev/log-viewer": "^5.0",
        "bmatovu/multi-auth": "^8.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.1",
        "laravel/passport": "^7.4",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "laravolt/indonesia": "^0.14.0",
        "nwidart/laravel-modules": "^5.0",
        "ramsey/uuid": "^3.8",
        "spatie/db-dumper": "^2.14",
        "spatie/laravel-backup": "^6.4",
        "spatie/laravel-permission": "^3.0",
        "yajra/laravel-datatables-oracle": "^9.6",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^1.0",
        "facade/ignition-code-editor": "^1.0",
        "filp/whoops": "^2.0",
        "fruitcake/laravel-telescope-toolbar": "^1.1",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helper/Helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
